{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "minecraft:mobspell_emitter",
      "basic_render_parameters": {
        "material": "particles_alpha",
        "texture": "textures/particle/particles"
      }
    },
    "components": {
      "minecraft:emitter_rate_instant": {
        "num_particles": 1
      },

      "minecraft:emitter_lifetime_once": {},

      "minecraft:emitter_shape_custom": {
        "direction": [ "Math.random(-0.20, 0.20)", "Math.random(0, 1)", "Math.random(-0.20, 0.20)" ]
      },

      "minecraft:particle_initial_speed": "(Math.random(0, 1) + Math.random(0, 1) + 1) * 0.50",

      "minecraft:particle_lifetime_expression": {
        "max_lifetime": 0.36
      },

      "minecraft:particle_motion_dynamic": {
        "linear_acceleration": [
          "(variable.particle_random_1 * 2 - 1)*0.5 - (variable.particle_age * 0.4) * 2.5",
          "1 + (0.08 * variable.particle_age) - (variable.particle_age * 0.4) * 1.5",
          "(variable.particle_random_2 * 2 - 1)*0.5 - (variable.particle_age * 0.4) * 2.5"
        ]
      },

      "minecraft:particle_appearance_billboard": {
        "size": [ 0.100, 0.100 ],

        "uv": {
          "texture_width": 128,
          "texture_height": 128,

          "flipbook": {
            "base_UV": [ 64, 64 ],
            "size_UV": [ 8, 8 ],
            "step_UV": [ -8, 0 ],
            "frames_per_second": 10,
            "max_frame": 8,
            "stretch_to_lifetime": true,
            "loop": false
          }
        }
      },
      "minecraft:particle_appearance_tinting": {
        "color": [ "variable.color.r", "variable.color.g", "variable.color.b", "variable.color.a" ]
      },
      "minecraft:particle_appearance_lighting": {}
    }
  }
}
