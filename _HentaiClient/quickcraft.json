  "tyel_quick_loot_mode_toggle@common_tabs.left_tab_toggle_with_custom_control": {
    "size": [
      100,
      28
    ],
    "$toggle_on_control": "tyel_common.quick_loot_mode_toggle_on_control",
    "$toggle_off_control": "tyel_common.quick_loot_mode_toggle_off_control",
    "$toggle_on_hover_control": "tyel_common.quick_loot_mode_toggle_on_hover_control",
    "$toggle_off_hover_control": "tyel_common.quick_loot_mode_toggle_off_hover_control",
