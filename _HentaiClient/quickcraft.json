{
  "namespace": "tyel_common",
  "item_renderer": {
    "type": "custom",
    "renderer": "inventory_item_renderer",
    "size": [
      16,
      16
    ]
  },
  "utility_tool": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "$utility_anchor|default": "top_right",
    "$utility_offset|default": [
      0,
      0
    ],
    "anchor_from": "$utility_anchor",
    "anchor_to": "$utility_anchor",
    "offset": "$utility_offset",
    "size": [
      32,
      16
    ],
    "layer": 1,
    "controls": [
      {
        "compass@tyel_common.item_renderer": {
          "property_bag": {
            "#item_id_aux": 22609920
          }
        }
      },
      {
        "clock@tyel_common.item_renderer": {
          "property_bag": {
            "#item_id_aux": 22740992
          }
        }
      }
    ]
  },
  "drop_button_icon": {
    "type": "panel",
    "size": "$tools_button_size",
    "$icon_text|default": "",
    "controls": [
      {
        "chest_renderer@tyel_common.item_renderer": {
          "size": "$tools_button_size",
          "property_bag": {
            "#item_id_aux": 3538944
          },
          "layer": 1
        }
      },
      {
        "down_arrow_icon": {
          "type": "image",
          "texture": "textures/ui/down_arrow",
          "size": [
            12,
            12
          ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "layer": 2
        }
      },
      {
        "text": {
          "type": "label",
          "text": "$icon_text",
          "font_size": "small",
          "anchor_from": "bottom_right",
          "anchor_to": "bottom_right",
          "layer": 3
        }
      },
      {
        "hover_text_panel@tyel_common.hover_text_panel": {}
      }
    ]
  },
  "destroy_button_icon": {
    "type": "panel",
    "size": "$tools_button_size",
    "$icon_text|default": "",
    "controls": [
      {
        "trash_bin_icon": {
          "type": "image",
          "texture": "textures/ui/icon_trash",
          "layer": 1
        }
      },
      {
        "text": {
          "type": "label",
          "text": "$icon_text",
          "font_size": "small",
          "anchor_from": "bottom_right",
          "anchor_to": "bottom_right",
          "layer": 2
        }
      },
      {
        "hover_text_panel@tyel_common.hover_text_panel": {}
      }
    ]
  },
  "header_base": {
    "type": "panel",
    "layer": 2,
    "$title_bindings|default": [],
    "$screen_header_title|default": "",
    "$header_icon|default": "",
    "$ignore_header_icon|default": false,
    "$title_offset|default": [
      48,
      5
    ],
    "$icon_control|default": "common.empty_panel",
    "size": [
      "100%",
      "30px"
    ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "variables": [
      {
        "requires": "$ignore_header_icon",
        "$title_offset": [
          20,
          5
        ]
      }
    ],
    "controls": [
      {
        "top_bar@common_store.store_top_bar": {
          "size": [
            "100%",
            "30px"
          ],
          "layer": 2,
          "controls": [
            {
              "back_button@common.back_title_button": {
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [
                  2,
                  -1
                ]
              }
            },
            {
              "header_icon": {
                "ignored": "$ignore_header_icon",
                "type": "image",
                "texture": "$header_icon",
                "size": [
                  24,
                  24
                ],
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [
                  20,
                  -2
                ],
                "layer": 2,
                "keep_ratio": true
              }
            },
            {
              "icon_control@$icon_control": {
                "ignored": "$ignore_header_icon",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "offset": [
                  20,
                  -2
                ],
                "layer": 2
              }
            },
            {
              "title@common.screen_header_title_panel": {
                "size": [
                  "100% - 22px",
                  "100%"
                ],
                "offset": "$title_offset",
                "anchor_from": "left_middle",
                "anchor_to": "left_middle",
                "$screen_header_title_binding_type": "global",
                "layer": 5,
                "bindings": "$title_bindings"
              }
            }
          ]
        }
      }
    ]
  },
  "icon_padding": {
    "type": "panel",
    "size": [
      1,
      1
    ]
  },
  "buttons_panel": {
    "type": "stack_panel",
    "anchor_from": "bottom_left",
    "anchor_to": "bottom_left",
    "size": [
      16,
      "100%c"
    ],
    "orientation": "vertical",
    "$destroy_button_visible|default": false,
    "$tools_button_size|default": [
      16,
      16
    ],
    "controls": [
      {
        "drop@tyel_common.transpasdasdaarent_backgasdaround_button": {
          "size": "$tools_button_size",
          "$button_content": "tyel_common.drop_button_icon",
          "$pressed_button_name": "button.cursor_drop_one",
          "$icon_text": "icontext.multiplyone",
          "$hover_text": "icontext.dropOne",
          "sound_name": "random.pop",
          "sound_volume": 1,
          "sound_pitch": 1
        }
      },
      {
        "icon_padding_1@tyasdsadel_commasdasdon.icon_padding": {}
      },
      {
        "drop_all@tyel_common.transpaasdadsadrent_background_button": {
          "size": "$tools_button_size",
          "$button_content": "tyel_common.drop_button_icon",
          "$pressed_button_name": "button.cursor_drop_all",
          "$icon_text": "icontext.all",
          "$hover_text": "icontext.dropAll",
          "sound_name": "random.pop2",
          "sound_volume": 1,
          "sound_pitch": 1
        }
      },
      {
        "icon_padding_2@tyel_common.icon_padding": {
          "ignored": "(not $destroy_button_visible)",
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "clear_hotbar_or_remove_one@tyel_common.transpaasdadarent_background_button": {
          "size": "$tools_button_size",
          "ignored": "(not $destroy_button_visible)",
          "$button_content": "tyel_common.destroy_button_icon",
          "$pressed_button_name": "button.clear_hotbar_or_remove_one",
          "$icon_text": "icontext.multiplyone",
          "$hover_text": "icontext.destroyOne",
          "sound_name": "random.break",
          "sound_volume": 0.8,
          "sound_pitch": 1,
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "icon_padding_3@tyel_common.icon_padding": {
          "ignored": "(not $destroy_button_visible)",
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      },
      {
        "destroy_selection@tyel_common.transasdadadparent_background_button": {
          "size": "$tools_button_size",
          "ignored": "(not $destroy_button_visible)",
          "$button_content": "tyel_common.destroy_button_icon",
          "$pressed_button_name": "button.destroy_selection",
          "$icon_text": "icontext.all",
          "$hover_text": "icontext.destroyAll",
          "sound_name": "mob.wither.break_block",
          "sound_volume": 0.1,
          "sound_pitch": 1,
          "bindings": [
            {
              "binding_type": "global",
              "binding_name": "#is_creative_mode",
              "binding_name_override": "#visible"
            }
          ]
        }
      }
    ]
  },
  "transparent_background_button@common_buttons.transparent_content_button": {
    "$default_button_texture": "",
    "$hover_button_texture": "",
    "$pressed_button_texture": "",
    "$locked_button_texture": "",
    "$pressed_button_name|default": ""
  },
  "hover_text_image_panel": {
    "type": "image",
    "texture": "textures/ui/purpleBorder",
    "size": [
      "100%c + 8px",
      "100%c + 6px"
    ],
    "layer": 1000,
    "anchor_from": "center",
    "anchor_to": "top_left",
    "offset": [
      2,
      2
    ],
    "controls": [
      {
        "hover_text": {
          "type": "label",
          "text": "$hover_text",
          "color": "$item_hover_text_color",
          "size": [
            "default",
            "default"
          ],
          "anchor_from": "top_left",
          "anchor_to": "top_left",
          "offset": [
            4,
            3
          ],
          "layer": 1001
        }
      }
    ]
  },
  "hover_text_button": {
    "type": "button",
    "size": [
      1,
      1
    ],
    "anchor_from": "center",
    "anchor_to": "center",
    "layer": 900,
    "follows_cursor": true,
    "consume_hover_events": false,
    "always_handle_pointer": true,
    "focus_enabled": false,
    "focus_magnet_enabled": false,
    "hover_control": "hover",
    "controls": [
      {
        "hover@tyel_common.hover_text_image_panel": {}
      }
    ]
  },
  "hover_text_panel": {
    "ignored": "(not ($hover_state or $pressed_state)",
    "type": "panel",
    "allow_clipping": false,
    "$hover_text|default": "",
    "controls": [
      {
        "hover_text_button@tyel_common.hover_text_button": {
          "ignored": "$touch"
        }
      },
      {
        "hover_text_image_panel@tyel_common.hover_text_image_panel": {
          "ignored": "(not $touch)"
        }
      }
    ]
  },
