  "music_player_album_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "album_title@music_player.music_player_title": {
          "$text": "tab.music_player.album.section"
        }
      },
      {
        "small_padding@how_to_play_common.small_padding": {}
      },
      {
        "volume_alpha@music_player.music_player_button_volume_alpha_base": {
          "$identifier": "tab.music_player.album.volume_alpha", //Non-custom songs only have "$identifier" variables. Since the player is not able to give them custom titles, the "$title" variable is unnecessary.
          "$sound_event|default": "album.alpha.ui",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ],
          "variables": [
            {
              "requires": "(not $is_music_plus)",
              "$sound_event": "album.alpha_native.ui" //If the active pack is not Music+, a random song from an alternate version of this album is selected. This album only includes songs that are in Minecraft by default. This is done to avoid the chance of a nonexistent sound file being called by the music player.
            }
          ],
          "$section_top": true
        }
      },
      {
        "volume_beta@music_player.music_player_button_volume_beta_base": {
          "$identifier": "tab.music_player.album.volume_beta",
          "$sound_event|default": "album.beta.ui",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ],
          "variables": [
            {
              "requires": "(not $is_music_plus)",
              "$sound_event": "album.beta_native.ui"
            }
          ]
        }
      },
      {
        "update_aquatic@music_player.music_player_button_update_aquatic_base": {
          "$identifier": "tab.music_player.album.update_aquatic",
          "$sound_event": "album.update_aquatic.ui",
          "$album_image_panel_controls": [ //The three Update Aquatic music tracks have unique covers. I chose to represent the album with the cover of Dragon Fish because the image is vaguely cube-shaped, which matches the covers of Volumes Alpha and Beta.
            {
              "dragon_fish@music_player.dragon_fish_cover": {}
            }
          ],
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ] //Some albums don't have a native variant. In the case of the Update Aquatic and Nether Update albums, there are no songs which aren't included in Minecraft by default, so a native album is unnecessary.
        }
      },
      {
        "nether_update@music_player.music_player_button_nether_update_base": {
          "$identifier": "tab.music_player.album.nether_update",
          "$sound_event": "album.nether_update.ui",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ]
        }
      },
      {
        "caves_and_cliffs@music_player.music_player_button_caves_and_cliffs_base": {
          "$identifier": "tab.music_player.album.caves_and_cliffs",
          "$sound_event": "album.caves_and_cliffs.ui",
          "$artist": "tab.music_player.artist.lena_raine_and_kumi_tanioka",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ],
          "variables": [
            {
              "requires": "(not $is_music_plus)",
              "$sound_event": "album.caves_and_cliffs_native.ui"
            }
          ]
        }
      },
      {
        "music_discs@music_player.music_player_song_button_panel_base": {
          "$identifier": "tab.music_player.album.music_discs",
          "$sound_event|default": "album.music_discs.ui",
          "$artist": "tab.music_player.artist.c418_and_lena_raine",
          "$album_image_panel_controls": [
            {
              "music_discs@music_player.music_discs_cover": {}
            }
          ],
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ],
          "variables": [
            {
              "requires": "(not $is_music_plus)",
              "$sound_event": "album.music_discs_native.ui",
              "$section_bottom": true //If Music+ songs aren't included, the album section ends here.
            }
          ]
        }
      },
      {
        "festive@music_player.music_player_button_festive_base": { //This album is called "Festive" instead of "Festive Remixes" because it also includes Flake and Kyoto from Volume Beta.
          "ignored": "(not $is_music_plus)",
          "$identifier": "tab.music_player.album.festive",
          "$sound_event": "album.festive.ui",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ] //This album doesn't have a native variant because none of its songs are included in Minecraft by default.
        }
      },
      {
        "minecraft_earth@music_player.music_player_button_minecraft_earth_base": {
          "ignored": "(not $is_music_plus)",
          "$identifier": "tab.music_player.album.minecraft_earth",
          "$sound_event": "album.minecraft_earth.ui",
          "$type_image_panel_controls": [
            {
              "shuffle@music_player.shuffle_image": {}
            }
          ],
          "$section_bottom": true
        }
      }
    ]
  },
  "music_player_volume_alpha_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "controls": [
      {
        "volume_alpha_title@music_player.music_player_title": {
          "$text": "tab.music_player.volume_alpha.section"
        }
      },
      {
        "small_padding@how_to_play_common.small_padding": {}
      },
      {
        "key@music_player.music_player_button_volume_alpha_base": { //Some songs don't have an "$artist" variable but still show the correct artist in-game. When an album is composed by only one person, the artist is set at the same level as the album cover.
          "$identifier": "tab.music_player.song.key",
          "$duration": "tab.music_player.duration.key", //Durations didn't strictly need to be translatable, but it's simplest to have all on-screen text located in the language files instead of the JSON code.
