//This object is the base of every song button in the music player. Its properties will be inherited by the objects in the above music player sections.
  "music_player_title@how_to_play_common.header": {
    "$body_text_color": "$section_title_text_color"
  },
  "music_player_song_button_panel_base": { //This object contains the basic button components that will be inherited by all player buttons used in the screen.
    "type": "panel",
    "size": [ "100%", 30 ],
    "$title|default": "", //As some buttons in the music player don't use these variables, setting default values prevents content errors that would otherwise appear.
    "$duration|default": "",
    "$duration_in_seconds|default": 0,
    "$album_image_offset|default": [ "50% - 16px", 0 ],
    "$type_image_offset|default": [ "-50% + 16px", 0 ],
    "controls": [
      {
        "album_image_panel": {
          "type": "panel",
          "offset": "$album_image_offset",
          "layer": 7,
          "controls": "$album_image_panel_controls" //This is made to be a variable so that it can be set to different values higher up depending on the player button type.
        }
      },
      {
        "type_image_panel": {
          "type": "panel",
          "offset": "$type_image_offset",
          "layer": 7,
          "controls": "$type_image_panel_controls",
          "$type_image_panel_controls|default": [ //The speaker image is set as the default control here. This is because it is used for almost every situation. The shuffle image is very scarcely used in comparison.
            {
              "speaker@music_player.speaker_image": {}
            }
          ]
        }
      },
      {
        "duration_bar_panel@music_player.duration_bar_properties": { //The duration bar properties set further down in this file are retrieved here.
          "type": "panel",
          "size": "@music_player.anim_duration_bar_show", //The duration bar panel size is set to an animation so that it can appear when pressed.
          "layer": 13,
          "anchor_to": "center",
          "anchor_from": "center",
          "offset": [ 0, 5 ],
          "clips_children": true, //The "clips_children" property forces all child controls to exist only within their parent panel. This is how the bar appears and disappears; its size is set to [ 182, 5 ] for the duration of the song, then back to [ 0, 0 ].
          "controls": [
            {
              "duration_bar_empty@$duration_bar_empty": { //The inherited properties for the duration bars are set to variables so that they can change depending on the user's settings. These variables are set by the "duration_bar_properties" object.
                "layer": 8,
                "controls": [
                  {
                    "duration_bar_scroller": {
                      "type": "panel",
                      "size": "@music_player.anim_duration_bar_fill", //The duration bar scroller size is set to an animation so that it can slowly expand and reveal the filled bar when pressed.
                      "layer": 12,
                      "anchor_to": "left_middle", //By anchoring the left middle of the duration bar to the left middle of the duration bar panel, the scrolling overlay will expand to the right as its size is changed during by animation.
                      "anchor_from": "left_middle",
                      "clips_children": true, //The "clips_children" property is used again by the scroller to limit the full duration bar to exist only within its boundaries.
                      "controls": [
                        {
                          "duration_bar_full@$duration_bar_full": {
                            "layer": 10
                          }
                        },
                        {
                          "duration_bar_overlay_full@$duration_bar_overlay_full": {
                            "layer": 11
                          }
                        }
                      ]
                    }
                  }
                ]
              }
            },
            {
              "duration_bar_overlay_empty@$duration_bar_overlay_empty": {
                "layer": 9
              }
            }
          ]
        }
      },
      {
        "music_player_button@settings_common.action_button": {
          "$border_visible": false, //Removing the button border is a necessity so that it can be recolored and hidden depending on the button state.
          "$button_text": "",
          "$default_button_texture|default": "textures/ui/empty", //This property and the three below are what give the player buttons their unique look.
          "$hover_button_texture|default": "textures/ui/focus_border_white",
          "$pressed_button_texture|default": "textures/ui/focus_border_green",
          "$locked_button_texture|default": "textures/ui/focus_border_red",
          "$focus_override_right": "FOCUS_OVERRIDE_STOP", //Since there are no buttons to the right of the player buttons, controller are blocked from being able to scroll to the right.
          "sound_name": "$sound_event", //This "$sound_event" variable is here so that each song can be given its own sound event.
          "sound_pitch": 1.0,
          "sound_volume": 1.0,
          "layer": 14,
          "$section_top|default": false,
          "$section_bottom|default": false,
          "variables": [
            {
              "requires": "((not $is_pregame) and $enable_sidebar and $section_top)", //If the sidebar is enabled, scrolling up at the top of a section will move the focus to the sidebar expand button.
              "$focus_override_up": "hamburger_toggle"
            },
            {
              "requires": "((not $is_pregame) and $enable_sidebar and $section_bottom)", //If the sidebar is enabled, the last button in each section will not allow the player to scroll down.
              "$focus_override_down": "FOCUS_OVERRIDE_STOP"
            }
          ],
          "button_mappings": [
            {
              "from_button_id": "button.menu_select",
              "to_button_id": "$sound_event", //Each music player button has its own duration bar. Each duration bar needs to appear separately, and only when its own button is pressed. Since every button has a unique "$sound_event" variable, it functions secondarily as the button's name.
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_secondary_select", //All these button mappings are required to support every input method.
              "to_button_id": "$sound_event",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_ok",
              "to_button_id": "$sound_event",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.controller_select",
              "to_button_id": "$sound_event",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.controller_secondary_select",
              "to_button_id": "$sound_event",
              "mapping_type": "pressed"
            }
          ]
        }
      },
      {
        "top_text": {
          "type": "label",
          "text": "$top_text",
          "$top_text|default": "$identifier", //The "$top_text" variable is never defined, except by other variables. This is because its purpose is to select between two possible texts: the hard-coded translatable identifier and the user-defined custom title.
          "color": "$song_button_top_text_color",
          "size": [ "default", 10 ],
          "max_size": [ "80%", "100%" ], //This variable is set so that the text will not overlap the album or speaker panels.
          "offset": "$top_text_offset", //The offset is defined by a variable because the top text will be centered on the button if an artist and duration are not given, and raised if they are.
          "layer": 28,
          "anchor_from": "center",
          "anchor_to": "center",
          "shadow": true, //This shadow helps differentiate the song titles from the artist text just a little more.
          "$top_text_offset|default": [ 0, -5 ],
          "variables": [
            {
              "requires": "(($artist = '') and ($duration = ''))", //This determinant requires that neither the artist nor duration are defined.
              "$top_text_offset": "@music_player.anim_top_text_shift" //The top text offset is set to an animation when the determinant is fulfilled. This animation keeps the text centered normally, but shifts the text upward for the duration of the song when pressed, ensuring that the text doesn't overlap the duration bar.
            },
            {
              "requires": "(not ($title = ''))", //This determinant checks whether the user has defined a custom title for the song. If they have, the "$top_text" variable will be set to "$title" instead of "$identifier".
              "$top_text": "$title"
            }
          ]
        }
      },
      {
        "bottom_text": {
          "type": "panel",
          "size": "$bottom_text_size", //The bottom text size is set to a variable because it needs to be able to disappear and reappear, but also stay visible if the song doesn't have a "duration_in_seconds" set.
          "layer": 27,
          "offset": "$bottom_text_offset", //The bottom text offset is set to a variable for the rare case when it takes the place of the top text. This is an impossible scenario outside of editing this file intentionally, but it is accounted for for the sake of full preparedness.
          "anchor_from": "top_middle",
          "anchor_to": "top_middle",
          "clips_children": true, //The "clips_children" property is used here to allow the bottom text to be hidden when the duration bar appears.
          "$bottom_text_size|default": "@music_player.anim_bottom_text_hide", //This animation will hide the bottom text when the button is pressed.
          "$bottom_text_offset|default": [ 0, "50%" ],
          "controls": [
            {
              "bottom_text_stack_panel": {
                "type": "stack_panel", //Since the duration and artist are separate values that appear on the same line, they have to be arranged by a stack panel.
                "max_size": [ "80%", "100%" ],
                "orientation": "horizontal",
                "controls": [
                  {
                    "duration": {
                      "type": "label",
                      "text": "$duration",
                      "color": "$song_button_bottom_text_color",
                      "size": [ "default", 10 ],
                      "layer": 26,
                      "ignored": "($duration = '')" //This determinant will hide the duration if it hasn't been set.
                    }
                  },
                  {
                    "spacer": {
                      "type": "label",
                      "text": " • ", //The spacer is not translatable because it doesn't need to be. It isn't used as text so much as an image that separates two texts.
                      "color": "$song_button_bottom_text_color",
                      "size": [ "default", 10 ],
                      "layer": 25,
                      "ignored": "(($artist = '') or ($duration = ''))" //This determinant requires that both an artist and duration must be set for the spacer to appear.
                    }
                  },
                  {
                    "artist": {
                      "type": "label",
                      "text": "$artist",
                      "color": "$song_button_bottom_text_color",
                      "size": [ "default", 10 ],
                      "layer": 26,
                      "ignored": "($artist = '')"
                    }
                  }
                ]
              }
            }
          ],
          "variables": [
            {
              "requires": "(($title = '') and ($identifier = '') and (not $duration_in_seconds = 0))", //This scenario is not possible outside of editing this file intentionally.
              "$bottom_text_size": [ "80%", "100%" ],
              "$bottom_text_offset": "@music_player.anim_bottom_text_shift", //The bottom text offset is set to an animation when the determinant is fulfilled. This animation keeps the text centered normally, but shifts the text upward for the duration of the song when pressed, ensuring that the text doesn't overlap the duration bar.
              "$song_button_bottom_text_color": "$song_button_top_text_color"
            }
          ]
        }
      }
    ]
  },
//This object is the base of the emote buttons. As emotes require their own property bags, bindings, and button mappings, it's easier to give them their own base.
  "music_player_emote_button_panel_base": {
    "type": "panel",
    "size": [ "100%", 30 ],
    "$emote_image_offset|default": [ "50% - 16px", 0 ],
    "$type_image_offset|default": [ "-50% + 16px", 0 ],
    "controls": [
      {
        "emote_image_panel": {
          "type": "panel",
          "offset": "$emote_image_offset",
          "layer": 7,
          "$property_bag": { //The property bag allows the object and all child objects to use specific bindings required by emotes.
            "#index": "$emote_slot_index"
          },
          "controls": [ //Controls are not variables for emotes because emote buttons always use the same images.
            {
              "emote_render@persona.emote_image": {}
            }
          ]
        }
      },
      {
        "type_image_panel": {
          "type": "panel",
          "offset": "$type_image_offset",
          "layer": 7,
          "controls": [
            {
              "emote_image@music_player.emote_image": {}
            }
          ]
        }
      },
      {
        "music_player_button@settings_common.action_button": {
          "$border_visible": false,
          "$button_text": "",
          "$default_button_texture|default": "textures/ui/empty",
          "$hover_button_texture|default": "textures/ui/focus_border_white",
          "$pressed_button_texture|default": "textures/ui/focus_border_green",
          "$locked_button_texture|default": "textures/ui/focus_border_red",
          "$focus_override_right": "FOCUS_OVERRIDE_STOP",
          "layer": 14,
          "property_bag": { //This property bag allows the buttons to imitate the six slots of the emote wheel. By setting the "$emote_slot_index" for each button, they are assigned matching emotes.
            "#hover_slice": "$emote_slot_index"
          },
          "$section_top|default": false,
          "$section_bottom|default": false,
          "variables": [
            {
              "requires": "((not $is_pregame) and $enable_sidebar and $section_top)",
              "$focus_override_up": "hamburger_toggle"
            },
            {
              "requires": "((not $is_pregame) and $enable_sidebar and $section_bottom)",
              "$focus_override_down": "FOCUS_OVERRIDE_STOP"
            }
          ],
          "button_mappings": [
            {
              "from_button_id": "button.menu_select", //These button mappings tell the game that an emote has been selected. Since the above property bag assigns each button an emote, the game plays the emote corresponding to the "$emote_slot_index" of the button and closes the menu.
              "to_button_id": "button.emote_selected",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_secondary_select",
              "to_button_id": "button.emote_selected",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.menu_ok",
              "to_button_id": "button.emote_selected",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.controller_select",
              "to_button_id": "button.emote_selected",
              "mapping_type": "pressed"
            },
            {
              "from_button_id": "button.controller_secondary_select",
              "to_button_id": "button.emote_selected",
              "mapping_type": "pressed"
            }
          ]
        }
      },
      {
        "no_emote_text": { //If the player has an empty emote slot, this text is applied.
          "type": "label",
          "text": "dr.none_button_text", //It would be best if this text said "Emote 1", "Emote 2", etc. instead of simply "None", but as far as I can tell, that's only possible in the character creator screen.
          "color": "$song_button_top_text_color", //Though the emotes aren't technically song buttons, they are intended to look similar, and are affected equally by the Customizer.
          "size": [ "default", 10 ],
          "max_size": [ "80%", "100%" ],
          "layer": 28,
          "anchor_from": "center",
          "anchor_to": "center",
          "shadow": true,
          "property_bag": {
            "#index": "$emote_slot_index"
          },
          "bindings": [
            {
              "binding_name": "(not #emote_is_valid)", //This binding is only possible because of "#index" in the property bag, which allows the game to determine whether or not the button has an emote assigned to it.
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ]
        }
      },
      {
        "emote_text": {
          "type": "label",
          "text": "#text",
          "color": "$song_button_top_text_color",
          "size": [ "default", 10 ],
          "max_size": [ "80%", "100%" ],
          "layer": 28,
          "anchor_from": "center",
          "anchor_to": "center",
          "shadow": true,
          "property_bag": {
            "#index": "$emote_slot_index"
          },
          "bindings": [
            {
              "binding_name": "#emote_name_touch", //Only the touch emote name binding allows the buttons to have different names at the same time. That's what we want, as unlike in the default emote screen, the music player adaptation shows all emotes at once.
              "binding_name_override": "#text",
              "binding_condition": "always"
            },
            {
              "binding_name": "#emote_is_valid", //Theoretically, this binding should never be necessary. Having it anyway ensures this element will never appear at the same time as the "no_emote_text" element.
              "binding_name_override": "#visible",
              "binding_condition": "always"
            }
          ]
        }
      }
    ]
  },
