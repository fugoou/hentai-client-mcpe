//This JSON was modified by AgentMindStorm (@agentms_) and Creep (@KillerArt_). Please do not use this document without permission.
//Since this file is quite complicated and others might want to understand it better to implement it in their own projects, I'll be leaving notes throughout the code to explain what's what.
{
  "namespace": "music_player",
//These objects are used to create the scrolling panel in the emote screen.
  "music_player_dialog@common_dialogs.main_panel_no_buttons": { //This is the background container for the music player. Without it, the list would fill the screen and be more difficult to read.
    "size": [ "65%", "95%" ],
    "$text_name": "tab.music_player.name",
    "$panel_indent_size":  [ "100% - 23px", "100% - 31px" ],
    "$child_control": "music_player.music_player_layered_panel"
  },
  "music_player_scrolling_panel@common.scrolling_panel": {
    "$touch": false, //Overriding the game's default global "$touch" variable with "false" allows the scroll bar to appear even on mobile devices.
    "$scrolling_pane_offset": [ 2, 0 ], //This offset moves the scroll bar ever-so-slightly more to the right to cover up the padding that comes with the dialog template.
    "$scroll_size": [ 15, "100%" ], //This variable widens the selectable area of the scroll bar.
    "$scroll_bar_left_padding_size": [ 0, 0 ], //These two padding reductions remove the empty space between the scroll bar and the buttons. This helps cut down on potential frustration by increasing the chance of selecting the scroll box instead of the empty space around it.
    "$scroll_bar_right_padding_size": [ 0, 0 ],
    "$scroll_box_mouse_image_control": "music_player.music_player_scrollbar_box_image", //The music player uses a wider scroll bar than normal in both mouse and touch modes. It's easier for touch players to grab consistently this way.
    "$scrolling_content|default": "music_player.sidebar_music_player_stack_panel", //This variable is where the entire music player object below is made scrollable.
    "$show_background": false, //A solid background works well in the settings screen, but the nature of the smaller, more accessible in-game music player causes it to work better semitransparent.
    "variables": [
      {
        "requires": "(not $enable_sidebar)", //This replaces the sidebar scrolling content with a single list if the player disables the sidebar. Controlled by the Customizer.
        "$scrolling_content": "music_player.plain_music_player_stack_panel" 
      }
    ]
  },
  "music_player_scrollbar_box_image@common.scrollbar_box_image": {
    "size": [ 15, "100%" ] //This widens the visual scroll bar.
  },
//This object layers the invisible background, scrolling content, and sidebar so they can all be inserted into the dialog at once.
  "music_player_layered_panel@customizer.custom_properties": { //This line hooks into the Customizer file to retrieve all the variables set by users. Variables in this section will be marked if they are set in the Customizer file.
    "type": "panel",
    "controls": [
      {
        "sidebar_safe_area": {
          "type": "stack_panel",
          "size": [ "100%", "100%" ],
          "orientation": "horizontal",
          "controls": [
            {
              "sidebar_padding": { //This makes space for the sidebar in the dialog screen.
                "type": "panel",
                "size": [ 28.25, 0 ],
                "ignored": "(not $enable_sidebar)" //The padding is unnecessary if the sidebar is absent.
              }
            },
            {
              "music_player_scrolling_panel@music_player.music_player_scrolling_panel": {
                "size": [ "fill", "100%" ]
              }
            }
          ]
        }
      },
      {
        "sidebar_panel@sidebar_navigation.sidebar_panel": {
          "offset": [ -3.5, 0 ], //This offset property pushes the sidebar into the safe zone of the dialog.
          "ignored": "(not $enable_sidebar)",
          "$sidebar_content": "music_player.sidebar_toggles_stack_panel",
          "$verbose_content": "music_player.sidebar_toggles_stack_panel", //Verbose toggles could use a separate toggle list, but that isn't necessary for the music player.
          "$dark_overlay_size": [ "100% + 7px", "100%" ], //The extra seven pixels in the overlay covers the safe zone of the dialog.
          "$dark_overlay_offset": [ 3.5, 0 ] //This offest ensures the dark overlay anchored to the center of the dialog, even though the sidebar is not.
        }
      },
      {
        "music_player_invisible_background@music_player.music_player_invisible_background": {}
      }
    ]
  },
  "music_player_invisible_background": { //This element is used as the "$screen_bg_content" in the emote screen. It absorbs selections made outside the buttons to stop misclicks from closing the screen.
    "type": "input_panel",
    "layer": 0,
    "size": [ "100%", "100%" ],
    "anchor_from": "center",
    "anchor_to": "center",
    "button_mappings": [
      {
        "from_button_id": "button.menu_select",
        "to_button_id": "button.null_select", //This button ID is unused, so selecting the invisible background does nothing.
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_secondary_select",
        "to_button_id": "button.null_select",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.menu_ok",
        "to_button_id": "button.null_select",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_select",
        "to_button_id": "button.null_select",
        "mapping_type": "pressed"
      },
      {
        "from_button_id": "button.controller_secondary_select",
        "to_button_id": "button.null_select",
        "mapping_type": "pressed"
      }
    ]
  },
