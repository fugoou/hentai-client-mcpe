  "plain_music_player_stack_panel@customizer.custom_properties": { //If players choose to disable the sidebar, this object is used as the scrolling content instead.
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "$album_image_offset": [ "-50% + 16px", 0 ], //These offset variables put the album cover or emote image on the left and the type image on the right, so the screen is visually balanced better.
    "$emote_image_offset": [ "-50% + 16px", 0 ],
    "$type_image_offset": [ "50% - 16px", 0 ],
    "bindings": [
      {
        "binding_type": "view",
        "source_control_name": "music_player_button_toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
      }
    ],
