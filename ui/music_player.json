//These objects are the animations that control object visibility.
  "anim_bottom_text_hide": {
    "anim_type": "size",
    "easing": "linear",
    "duration": 0, //Hide and show animations are performed instantly. These animations are not meant to be the focus.
    "from": [ "80%", "100%" ],
    "to": [ 0, 0 ],
    "play_event": "$sound_event", //The "$sound_event" variables double as unique button names, so the animations only trigger on the button that was pressed.
    "next": "@music_player.anim_bottom_text_wait"
  },
  "anim_bottom_text_wait": {
    "anim_type": "wait", //This animation's purpose is to wait for the song to be over, then move on to the next animation.
    "duration": "$duration_in_seconds",
    "next": "@music_player.anim_bottom_text_show"
  },
  "anim_bottom_text_show": {
    "anim_type": "size",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ "80%", "100%" ]
  },
  "anim_bottom_text_shift": { //This animation chain will only be used if you remove a song identifier from this file.
    "anim_type": "offset",
    "easing": "linear",
    "duration": 0,
    "from": [ 0, "35%" ],
    "to": [ 0, "17.5%" ],
    "play_event": "$sound_event",
    "next": "@music_player.anim_bottom_text_stay"
  },
  "anim_bottom_text_stay": { //This animation is functionally the same as the "anim_bottom_text_wait" animation, but it had to be separate because it's part of a different animation chain.
    "anim_type": "wait",
    "duration": "$duration_in_seconds",
    "next": "@music_player.anim_bottom_text_return"
  },
  "anim_bottom_text_return": {
    "anim_type": "offset",
    "duration": 0,
    "from": [ 0, "17.5%" ],
    "to": [ 0, "35%" ]
  },
  "anim_duration_bar_show": {
    "anim_type": "size",
    "easing": "linear",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 182, 5 ],
    "play_event": "$sound_event",
    "next": "@music_player.anim_duration_bar_wait"
  },
  "anim_duration_bar_wait": {
    "anim_type": "wait",
    "duration": "$duration_in_seconds",
    "next": "@music_player.anim_duration_bar_hide"
  },
  "anim_duration_bar_hide": {
    "anim_type": "size",
    "duration": 0,
    "from": [ 182, 5 ],
    "to": [ 0, 0 ]
  },
  "anim_duration_bar_fill": { //This fill animation is the only animation with movement that takes a nonzero amount of time to complete. That's due to the duration bar's filling motion being the focus of the animations.
    "anim_type": "size",
    "easing": "linear",
    "duration": "$duration_in_seconds",
    "from": [ 0, 5 ],
    "to": [ 182, 5 ],
    "play_event": "$sound_event",
    "next": "@music_player.anim_duration_bar_reset"
  },
  "anim_duration_bar_reset": { //This reset animation is technically unnecessary, but it would be necessary if the duration bar could appear on the same button multiple times.
    "anim_type": "size",
    "easing": "linear",
    "duration": 0,
    "from": [ 182, 5 ],
    "to": [ 0, 5 ]
  },
  "anim_top_text_shift": {
    "anim_type": "offset",
    "easing": "linear",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 0, -5 ],
    "play_event": "$sound_event",
    "next": "@music_player.anim_top_text_wait"
  },
  "anim_top_text_wait": {
    "anim_type": "wait",
    "duration": "$duration_in_seconds",
    "next": "@music_player.anim_top_text_return"
  },
  "anim_top_text_return": {
    "anim_type": "offset",
    "duration": 0,
    "from": [ 0, -5 ],
    "to": [ 0, 0 ]
  },
