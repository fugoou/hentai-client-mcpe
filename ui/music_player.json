//These objects control the album covers.
  "cover_border": { //This border surrounds the album cover.
    "type": "image",
    "size": [ 18, 18 ],
    "layer": 8,
    "color": "$border_color",
    "$border_color|default": [ 1, 1, 1 ],
    "texture": "textures/ui/focus_border_white"
  },
  "cover_base": { //This is the base of the album cover. The properties set here will be inherited by each cover type.
    "type": "image",
    "size": [ 16, 16 ],
    "layer": 7,
    "texture": "$album_cover_texture",
    "$album_cover_texture|default": "textures/ui/cover_background_gray",
    "controls": [ //The controls only contain the border by default.
      {
        "border@music_player.cover_border": {}
      }
    ]
  },
  "custom_cover@music_player.cover_base": {
    "controls": [
      {
        "border@music_player.cover_border": {
          "layer": 11
        }
      },
      {
        "quaver": {
          "type": "image",
          "anchor_to": "center",
          "anchor_from": "center",
          "size": [ 8.3, 13.28 ], //These values were calculated by taking the size of an individual quaver texture and multiplying by 5/3.
          "uv_size": [ 5, 8 ],
          "uv": "$quaver_location", //The uv is set to a variable, which allows different songs to use different quavers.
          "layer": 9,
          "texture": "$quaver_texture",
          "$quaver_texture|default": "textures/ui/quaver",
          "$quaver_variant|default": "default",
          "variables": [
            {
              "requires": "($quaver_variant = 'upside_down')", //These are the determinants that select the quaver type. They look for a specific value of the "$quaver_variant" variable, which is set for each group of 25 custom songs.
              "$quaver_texture": "textures/ui/quaver_upside_down"
            },
            {
              "requires": "($quaver_variant = 'inverted')",
              "$quaver_texture": "textures/ui/quaver_inverted"
            },
