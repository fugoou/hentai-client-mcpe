// RTL Modified by TLGaming2308

{
  "namespace": "common",

  "screen_header_title_panel": {
    "type": "panel",
    "size": [ "fill", 17 ],
    "$screen_header_title_visible|default": true,
    "controls": [
      {
        "store_header_title": {
          "type": "label",
          "size": [ "100%", 17 ],
          "offset": [ 0, 3.5 ],
          "ignored": "(not $screen_header_title_visible)",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "text_alignment": "right",
          "color": "$title_text_color",
          "$screen_header_title|default": "#screen_header_title",
          "$screen_header_title_binding_type|default": "none",
          "text": "$screen_header_title",
          "bindings": [
            {
              "binding_type": "$screen_header_title_binding_type",
              "binding_name": "$screen_header_title",
              "binding_name_override": "$screen_header_title"
            }
          ]
        }
      }
    ]
  },
  
  "back_title_button": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "$button_text_binding_type|default": "none",
    "$back_button_has_title|default": false,
    "controls": [
      {
        "panel2": {
          "type": "panel",
          "size": [ "100%c", "100%" ],
          "ignored": "(not $back_button_has_title)",
          "controls": [
            {
              "label": {
                "type": "label",
                "layer": 2,
                "text": "$button_text",
                "color": "$title_text_color",
                "text_alignment": "center",
                "bindings": [
                  {
                    "binding_type": "$button_text_binding_type",
                    "binding_name": "$button_text",
                    "binding_name_override": "$button_text"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "padding1": {
          "type": "panel",
          "size": [ 3, "100%" ],
          "ignored": "(not $back_button_has_title)"
        }
      },
      {
        "panel1": {
          "type": "panel",
          "offset": [ 2, 0 ],
          "size": [ 16, "100%" ],
          "controls": [
            {
              "button@common.back_button": {
                "$button_text": "",
                "$button_content": "common.back_title_button_content",
                "$button_tts_header": "accessibility.button.back"
              }
            }
          ]
        }
      }
    ]
  },

  "back_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%c", 16 ],
    "offset": [ 0, 2 ],
    "controls": [
      {
        "label_panel": {
          "type": "panel",
          "size": [ "100%c", "100%c" ],
          "controls": [
            {
              "label@common_buttons.new_ui_binding_button_label": {
                "offset": [ 0, -2 ]
              }
            }
          ]
        }
      },
      {
        "padding1": {
          "type": "panel",
          "size": [ 4, "100%" ]
        }
      },
      {
        "chevron_panel": {
          "type": "panel",
          "size": [ "100%c", "100%c" ],
          "controls": [
            {
              "left_chevron@common.chevron_image": {
                "texture": "textures/ui/chevron_right",
                "offset": [ 0, -1 ],
                "color": "$text_color"
              }
            }
          ]
        }
      }
    ]
  },

  "back_title_button_content": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [ "100%c", 16 ],
    "offset": [ 0, 2 ],
    "controls": [
      {
        "padding1": {
          "type": "panel",
          "size": [ 4, "100%" ]
        }
      },
      {
        "chevron_panel": {
          "type": "panel",
          "size": [ "100%c", "100%c" ],
          "controls": [
            {
              "left_chevron@common.chevron_image": {
                "texture": "textures/ui/chevron_right",
                "offset": [ 0, -1 ],
                "color": "$text_color"
              }
            }
          ]
        }
      },
      {
        "padding2": {
          "type": "panel",
          "size": [ 4, "100%" ]
        }
      }
    ]
  },

  "back_content_panel": {
    "type": "panel",
    "size": [ "100%c", "100%" ],
    "anchor_from": "right_middle",
    "anchor_to": "right_middle",
    "controls": [
      {
        "back_button@common.back_title_button": {
          "offset": [ 0, -1 ],
          "layer": 200 //This should always be on top, let's ensure that
        }
      }
    ]
  },

  "fullscreen_header": {
    "type": "stack_panel",
    "size": [ "100%", "100%" ],
    "$child_control|default": "common.empty_panel",
    "controls": [
      {
        "top_bar@common.top_bar": {
          "size": [ "100%", 23 ],
          "controls": [
            {
              "title_stack_panel": {
                "type": "stack_panel",
                "size": [ "100%", 19 ],
                "orientation": "horizontal",
                "controls": [
                  {
                    "padding1": {
                      "type": "panel",
                      "size": [ "fill", "100%" ]
                    }
                  },
                  { "back_button_content_panel@common.back_content_panel": {} },
                  {
                    "padding2": {
                      "type": "panel",
                      "size": [ 2, "100%" ]
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "child_control@$child_control": {
          "size": [ "100%", "fill" ],
          "layer": 2
        }
      }
    ]
  },
  "anim_item_details_alpha": {
    "anim_type": "alpha",
    "easing": "in_quint",
    "destroy_at_end": "selected_item_details",
    "duration": 0,
    "from": 1.0,
    "to": 0
  },

  "anim_details_bubble_alpha": {
    "anim_type": "alpha",
    "easing": "in_quint",
    "destroy_at_end": "details_bubble_control",
    "duration": 0,
    "from": 1.0,
    "to": 0
  },

  "anim_item_lock_notification_background_alpha": {
    "anim_type": "alpha",
    "easing": "in_quint",
    "destroy_at_end": "selected_item_details",
    "duration": 0,
    "from": 0.6,
    "to": 0
  },

  "anim_item_lock_notification_text_alpha": {
    "anim_type": "alpha",
    "easing": "in_quint",
    "destroy_at_end": "selected_item_details",
    "duration": 0,
    "from": 1.0,
    "to": 0
  },

  "anim_scrollbar_box_fadeout": {
    "anim_type": "alpha",
    "play_event": "scrollbar.released",
    "reset_event": "scrollbar.active",
    "easing": "in_sine",
    "duration": 0,
    "from": 1,
    "to": 0,
    "next": "@common.anim_scrollbar_box_fadeout"
  },

  "screen_exit_size_animation_push": {
    "anim_type": "size",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ "100%", "100%" ],
    "to": [ 0, 0 ],
    "play_event": "screen.exit_push",
    "reset_event": "screen_animation_reset"
  },

  "screen_exit_size_animation_pop": {
    "anim_type": "size",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ "100%", "100%" ],
    "to": [ 0, 0 ],
    "play_event": "screen.exit_pop",
    "reset_event": "screen_animation_reset"
  },

  "screen_entrance_size_animation_immediate": {
    "anim_type": "size",
    "easing": "out_back",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ "100%", "100%" ],
    "resettable": false
  },

  "screen_entrance_size_animation_push@common.screen_entrance_size_animation_immediate": {
    "duration": 0,
    "play_event": "screen.entrance_push",
    "resettable": true
  },

  "screen_entrance_size_animation_pop@common.screen_entrance_size_animation_immediate": {
    "duration": 0,
    "play_event": "screen.entrance_pop",
    "resettable": true
  },

  "screen_exit_animation_push_fade": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "screen_exit_animation_pop_fade": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "screen_entrance_animation_push_fade": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 0.0,
    "to": 1.0,
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },

  "screen_entrance_animation_pop_fade": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 0.0,
    "to": 1.0,
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },

  "screen_exit_animation_push_offset": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ "-25%", 0 ],
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "screen_exit_animation_pop_offset": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ "25%", 0 ],
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "screen_entrance_animation_push_offset": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ "25%", 0 ],
    "to": [ 0, 0 ],
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },

  "screen_entrance_animation_pop_offset": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ "-25%", 0 ],
    "to": [ 0, 0 ],
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },

  "container_screen_exit_animation_push": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 0, "50%" ],
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "container_screen_exit_animation_pop": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, 0 ],
    "to": [ 0, "50%" ],
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "container_screen_entrance_animation_push": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, "50%" ],
    "to": [ 0, 0 ],
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },

  "container_screen_entrance_animation_pop": {
    "anim_type": "offset",
    "easing": "out_cubic",
    "duration": 0,
    "from": [ 0, "50%" ],
    "to": [ 0, 0 ],
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },

  "screen_exit_animation_push_alpha": {
    "anim_type": "alpha",
    "scale_from_starting_alpha": true,
    "easing": "linear",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_push",
    "end_event": "screen.exit_end"
  },

  "screen_exit_animation_pop_alpha": {
    "anim_type": "alpha",
    "scale_from_starting_alpha": true,
    "easing": "linear",
    "duration": 0,
    "from": 1.0,
    "to": 0.0,
    "play_event": "screen.exit_pop",
    "end_event": "screen.exit_end"
  },

  "screen_entrance_animation_push_alpha": {
    "anim_type": "alpha",
    "scale_from_starting_alpha": true,
    "easing": "linear",
    "duration": 0,
    "from": 0.0,
    "to": 1.0,
    "play_event": "screen.entrance_push",
    "end_event": "screen.entrance_end"
  },

  "screen_entrance_animation_pop_alpha": {
    "anim_type": "alpha",
    "scale_from_starting_alpha": true,
    "easing": "linear",
    "duration": 0,
    "from": 0.0,
    "to": 1.0,
    "play_event": "screen.entrance_pop",
    "end_event": "screen.entrance_end"
  },

  "loading_bar_init_animation_push": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 0.0,
    "to": 0.0,
    "play_event": "screen.exit_push",
    "next": "@common.loading_bar_wait_animation"
  },

  "loading_bar_init_animation_pop@common.loading_bar_init_animation_push": {
    "play_event": "screen.exit_pop"
  },

  "loading_bar_wait_animation": {
    "anim_type": "wait",
    "duration": 0,
    "next": "@common.loading_bar_fade_animation"
  },

  "loading_bar_fade_animation": {
    "anim_type": "alpha",
    "easing": "out_cubic",
    "duration": 0,
    "from": 0.0,
    "to": 1.0
  }
  
  
}